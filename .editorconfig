# EditorConfig: https://EditorConfig.org

root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

[*.{js,ts,styl,pug}]
indent_style = space
indent_size = 4
charset = utf-8

[{package.json,.travis.yml}]
indent_style = space
indent_size = 2
charset = utf-8
