/* Shared */

#bottomRowTextLabel {
    /* Installing at C:\Users\user\AppData\Local\ct.js */
    /* Pro tip: use the same installer to update ct.js! */

    position: absolute;
    width: 300px;
    height: 19px;
    left: 21px;
    top: calc(50% - 19px / 2 - 0.5px);

    font-family: Open Sans;
    font-style: normal;
    font-weight: normal;
    font-size: 14px;
    line-height: 19px;

    /* identical to box height */

    color: #666666;
}

#changeAbortLabel {
    /* Change... */
    /* Abort */

    position: absolute;
    width: 61px;
    height: 19px;
    left: calc(50% - 61px / 2);
    top: calc(50% - 19px / 2 - 0.5px);

    font-family: Open Sans;
    font-style: normal;
    font-weight: normal;
    font-size: 14px;
    line-height: 19px;

    /* identical to box height */

