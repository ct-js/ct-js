/* Welcome page */

#welcomeLabel {
    /* Welcome to Ct.js! */

    position: absolute;
    width: 248px;
    height: 44px;
    left: 20px;
    top: 46px;

    font-family: 'Open Sans', -apple-system, BlinkMacSystemFont, 'Segoe UI', 'Roboto Thin', Oxygen, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif;
    font-style: normal;
    font-weight: 100;
    font-size: 32px;
    line-height: 44px;

    /* identical to box height */

    color: #5144db;
}

#instructionsLabel {
    /* You're almost there! Press this big button to open a door to a wonderful world of game development: */

    position: absolute;
    width: 245px;
    height: 57px;
    left: 23px;
    top: 101px;

    font-family: Open Sans;
    font-style: normal;
    font-weight: normal;
    font-size: 14px;
    line-height: 19px;

    color: #666666;
}

#installButtonLabel {
    /* Install ct.js */

    position: absolute;
    width: 112px;
    height: 33px;
    left: calc(50% - 112px / 2 - 0.5px);
    top: calc(50% - 33px / 2 - 0.5px);

    font-family: Open Sans;
    font-style: normal;
    font-weight: 300;
    font-size: 24px;
    line-height: 33px;

    /* identical to box height */

    color: #ffffff;

    background-color: #446adb;
    border-width: 1px;
    border-style: solid;
    border-color: #5144db;
    border-radius: 3px;
}

#installButtonLabel:hover {
    color: #446adb;
    background-color: #ffffff;
    border-color: #999999;
}

