{
    "$schema": "https://hst.sh/raw/hapovibiwu",
    "applicationId": "rocks.ctjs.ide",
    "version": "5.2.0",
    "defaultMode": "window",
    "port": 0,
    "documentRoot": "/app/",
    "url": "/",
    "enableServer": true,
    "enableNativeAPI": true,
    "enableExtensions": true,
    "exportAuthInfo": true,
    "tokenSecurity": "one-time",
    "globalVariables": {},
    "logging": {
        "enabled": true,
        "writeToLogFile": true
    },
    "nativeAllowList": [
        "app.*",
        "os.*",
        "window.*",
        "events.*",
        "extensions.*",
        "filesystem.*",
        "debug.log",
        "storage.*"
    ],
    "modes": {
        "window": {
            "title": "ctjs",
            "width": 800,
            "height": 500,
            "minWidth": 400,
            "minHeight": 200,
            "center": true,
            "fullScreen": false,
            "alwaysOnTop": false,
            "icon": "/app/ct_ide.png",
            "enableInspector": true,
            "borderless": false,
            "maximize": true,
            "hidden": false,
            "resizable": true,
            "exitProcessOnClose": false
        }
    },
    "cli": {
        "binaryName": "ctjs",
        "resourcesPath": "/app/",
        "extensionsPath": "/backend/",
        "binaryVersion": "5.3.0",
        "clientVersion": "5.3.0",
        "clientLibrary": "/neutralinoClient/neutralino.js",
        "distributionPath": "/build",
        "autoReloadExclude": "\\.css$"
    },
    "extensions": [{
        "id": "extBun",
        "commandDarwin": "${NL_PATH}/backend/run ${NL_PATH}",
        "commandLinux": "${NL_PATH}/backend/run ${NL_PATH}",
        "commandWindows": "${NL_PATH}\\backend\\run.cmd ${NL_PATH}"
    }]
}
