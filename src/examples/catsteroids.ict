scripts:
  - name: calculateDamage
    code: |-
      const calculateDamage = function(ship, bullet) {
          ship.health -= bullet.damage;
          if (bullet.type === 'Laser_Bolt_Blue') { // Bolts may survive the collision do additional damage
              bullet.damage = -this.health; // bring overkill damage back to the bullet
              if (bullet.damage <= 0) {
                  bullet.kill = true;
              }
          } else {
              bullet.kill = true;
          }
          if (ship.health <= 0) {
              ship.kill = true;
          }
      }
  - name: spawnWigglers
    code: |-
      const spawnWigglers = function(startingPhase) {
          // create 5 enemies, one by one in 1.4 seconds
          ct.types.copy('Enemy_Wiggler', 0, -100, {
              phase: startingPhase
          });
          ct.u.wait(350)
          .then(() => {
              ct.types.copy('Enemy_Wiggler', 0, -100, {
                  phase: startingPhase
              });
          })
          .then(() => ct.u.wait(350))
          .then(() => {
              ct.types.copy('Enemy_Wiggler', 0, -100, {
                  phase: startingPhase
              });
          })
          .then(() => ct.u.wait(350))
          .then(() => {
              ct.types.copy('Enemy_Wiggler', 0, -100, {
                  phase: startingPhase
              });
          })
          .then(() => ct.u.wait(350))
          .then(() => {
              ct.types.copy('Enemy_Wiggler', 0, -100, {
                  phase: startingPhase
              });
          });
      };
