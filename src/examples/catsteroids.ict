  - name: spawnWigglers
    code: |-
      const spawnWigglers = function(startingPhase) {
          // create 5 enemies, one by one in 1.4 seconds
          templates.copy('Enemy_Wiggler', 0, -100, {
              phase: startingPhase
          });
          u.wait(350)
          .then(() => {
              templates.copy('Enemy_Wiggler', 0, -100, {
                  phase: startingPhase
              });
          })
          .then(() => u.wait(350))
          .then(() => {
              templates.copy('Enemy_Wiggler', 0, -100, {
                  phase: startingPhase
              });
          })
          .then(() => u.wait(350))
          .then(() => {
              templates.copy('Enemy_Wiggler', 0, -100, {
                  phase: startingPhase
              });
          })
          .then(() => u.wait(350))
          .then(() => {
              templates.copy('Enemy_Wiggler', 0, -100, {
                  phase: startingPhase
              });
          });
      };
