  - name: EnemyShip
    depth: 0
    oncreate: |-
      this.speed = 3;
      this.direction = 90;

      this.bulletTimer = 60;
      this.ctype = 'Hostile';
    onstep: |-
      this.move();

      if (this.y > ct.height + 80) {
          this.kill = true;
      }

      var collided = ct.place.meet(this, this.x, this.y, 'Laser_Blue');
      if (collided) {
          collided.kill = true;
          this.kill = true;
          ct.room.score += 100;
      }

      this.bulletTimer -= ct.delta;
      if (this.bulletTimer <= 0) {
          this.bulletTimer = 180;
          ct.types.copy('Laser_Red', this.x, this.y + 32);
      }
    ondraw: ''
