  - name: Laser_Blue
    depth: 0
    oncreate: |-
      this.speed = 18;
      this.direction = 270;
    onstep: |-
      if (this.y < -40) {
          this.kill = true;
      }

      ct.types.move(this);
    ondraw: ''
    ondestroy: ''
