  - name: EndGame_Star_Counter
    depth: 0
    oncreate: |-
      if (!('JettyCat_HighScore' in localStorage)) {
          localStorage['JettyCat_HighScore'] = ct.room.stars;
      } else if (localStorage['JettyCat_HighScore'] < ct.room.stars) {
          localStorage['JettyCat_HighScore'] = ct.room.stars;
      }

      var scoreText = `Your score: ${ct.room.stars}
      Highscore: ${localStorage['JettyCat_HighScore']}`;

      this.label = new PIXI.Text(scoreText, ct.styles.get('Orange'));
      this.label.anchor.x = this.label.anchor.y = 0.5;
